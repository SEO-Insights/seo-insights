{
    "manifest_version": 2,
    "name": "SEO Insights",
    "version": "0.0.1",
    "version_name": "0.0.1",
    "description": "A simple SEO extension to get insights of the current site.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html",
        "default_title": "SEO Insights"
    },
    "permissions": [
        "activeTab"
    ]
}